{{#partial "content"}}
    <a href="/Eanimals">Endangered Animals</a>
    <a href="/Wanimals">Wild Animals</a>

    <h3>Endangered Animals:</h3>
    <div class="row">
            {{#each Eanimal}}
        <div class="col-md-4">
            <div class="card">
                <div class="container">
                    <h3>Animal: {{name}} </h3>
                    <p>Health: {{healthLevel}}</p>
                    <p>Age: {{age}}</p>
                    <p>Id: {{ id }}</p>
                </div>
            </div>
        </div>
        {{/each}}
        <h3>Wild Animals: </h3>
        {{#each Wanimal}}
            <div class="col-md-4">
                <div class="card">
                    <div class="container">
                        <h3>Animal: {{name}} </h3>
                        <p>Id: {{ id }}</p>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>

</div>


{{/partial}}

{{> layout.hbs}}