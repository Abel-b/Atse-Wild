{{#partial "content"}}
    <h3>Endangered Animals: </h3>
    <div class="row">
                {{#each Eanimal}}
                    <div class="col-md-6">
                        <div class="card">
                            <div class="container">
                                <h3>Animal: {{name}} </h3>
                                <p>Health: {{health}}/10</p>
                                <p>Age: {{age}}/10</p>
                                <p>Id: {{ id }}</p>
                            </div>
                        </div>
                    </div>
                {{/each}}
        </div>
    </div>
{{/partial}}

{{> layout.hbs}}